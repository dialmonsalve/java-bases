package primerosPasos;

public class UsoArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int miMatriz[] = { 8, 7, 4, 7, 5, 9, 5, 4, 8, 6 };
		int miMatriz2[] = new int[5];
		
		miMatriz2[0] = 5;
		miMatriz2[1] = 5;
		miMatriz2[6 = 5;
		miMatriz2[0] = 5;
		miMatriz2[0] = 5;
		
		for (int i : miMatriz) {
			System.out.println(i);
		}
	}

}
