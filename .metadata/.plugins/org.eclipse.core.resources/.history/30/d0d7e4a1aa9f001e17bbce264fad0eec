package primerosPasos;



public class DeclaracionesOperadores {

	public static void main(String[] args) {
		int sumar(int a, int b) {
			return a + b;
		}
		// TODO Auto-generated method stub
		double a = 5;
		double b;
		b = 7;
		
		double c = a / b;
		final int d = 8;
		
		
		System.out.println(a == b);
		System.out.println(c);
	}

}
